<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:id="@+id/activity_symbo_popup_window"
    android:layout_width="match_parent"
    android:layout_height="fill_parent"
    android:background="#FFFFF0"
    android:orientation="vertical">

    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_marginTop="10dp"
        android:orientation="horizontal" >

        <TextView
            android:id="@+id/title"
            android:layout_width="0dp"
            android:layout_height="wrap_content"
            android:layout_gravity="center_vertical"
            android:layout_weight="1"
            android:layout_marginLeft="10dp"
            android:text="请输入特体数值："
            android:textSize="18sp" />
        <TextView
            android:layout_width="0dp"
            android:layout_height="wrap_content"
            android:layout_gravity="center_vertical"
            android:layout_weight="1"
            android:textSize="18sp"
            android:textColor="@color/colorAccent"
            android:id="@+id/print"/>
        <ImageView
            android:id="@+id/team_popupdel"
            android:layout_width="49dp"
            android:layout_height="42dp"
            android:layout_gravity="center_vertical"
            android:padding="10dp"
            android:src="@drawable/abc_ic_clear_search_api_holo_light" />

    </LinearLayout>


    <GridLayout
            android:layout_width="fill_parent"
            android:layout_height="wrap_content"
            android:layout_marginTop="10px"
            android:columnCount="5"
            android:rowCount="6">
            <Button
                android:id="@+id/add1"
                android:layout_width="@dimen/sybol_button_width"
                android:layout_height="wrap_content"
                android:layout_column="0"
                android:layout_row="1"
                android:text="+ 1"

                />

            <Button
                android:id="@+id/sub2"
                android:layout_width="@dimen/sybol_button_width"
                android:layout_height="wrap_content"
                android:layout_column="3"
                android:layout_row="1"

                android:text="- 2"

                />

            <Button
                android:id="@+id/sub1"
                android:layout_width="@dimen/sybol_button_width"
                android:layout_height="wrap_content"
                android:layout_column="1"
                android:layout_row="1"
                android:text="- 1"

                />

            <Button
                android:id="@+id/add2"
                android:layout_width="@dimen/sybol_button_width"
                android:layout_height="wrap_content"
                android:layout_column="2"
                android:layout_row="1"
                android:text="+ 2"

                />
            <Button
                android:id="@+id/back"
                android:layout_width="@dimen/sybol_button_width"
                android:layout_height="wrap_content"
                android:layout_column="4"
                android:layout_row="1"

                android:text="清除"
                />

            <Button
                android:id="@+id/txt7"
                android:layout_width="@dimen/sybol_button_width"
                android:layout_height="wrap_content"
                android:layout_column="0"
                android:layout_row="2"
                android:text="7"

                />

            <Button
                android:id="@+id/txt8"
                android:layout_width="@dimen/sybol_button_width"
                android:layout_height="wrap_content"
                android:layout_column="1"
                android:layout_row="2"
                android:text="8"

               />

            <Button
                android:id="@+id/txt9"
                android:layout_width="@dimen/sybol_button_width"
                android:layout_height="wrap_content"
                android:layout_column="2"
                android:layout_row="2"
                android:text="9"

                />

            <Button
                android:id="@+id/add3"
                android:layout_width="@dimen/sybol_button_width"
                android:layout_height="wrap_content"
                android:layout_column="3"
                android:layout_row="2"
                android:text="+ 3"
            />

            <Button
                android:id="@+id/sub3"
                android:layout_width="@dimen/sybol_button_width"
                android:layout_height="wrap_content"
                android:layout_column="4"
                android:layout_row="2"
                android:text="- 3"
                />

            <Button
                android:id="@+id/txt4"
                android:layout_width="@dimen/sybol_button_width"
                android:layout_height="wrap_content"
                android:layout_column="0"
                android:layout_row="3"
                android:text="4"
                />

            <Button
                android:id="@+id/txt5"
                android:layout_width="@dimen/sybol_button_width"
                android:layout_height="wrap_content"
                android:layout_column="1"
                android:layout_row="3"
                android:text="5"
                />

            <Button
                android:id="@+id/txt6"
                android:layout_width="@dimen/sybol_button_width"
                android:layout_height="wrap_content"
                android:layout_column="2"
                android:layout_row="3"
                android:text="6"

                />

            <Button
                android:id="@+id/add05"
                android:layout_width="@dimen/sybol_button_width"
                android:layout_height="wrap_content"
                android:layout_column="3"
                android:layout_row="3"
                android:text="+ 0.5"
                />

            <Button
                android:id="@+id/sub05"
                android:layout_width="@dimen/sybol_button_width"
                android:layout_height="wrap_content"
                android:layout_column="4"
                android:layout_row="3"
                android:text="- 0.5"
               />

            <Button
                android:id="@+id/txt1"
                android:layout_width="@dimen/sybol_button_width"
                android:layout_height="wrap_content"
                android:layout_column="0"
                android:layout_row="4"
                android:text="1"
                />

            <Button
                android:id="@+id/txt2"
                android:layout_width="@dimen/sybol_button_width"
                android:layout_height="wrap_content"
                android:layout_column="1"
                android:layout_row="4"
                android:text="2"
                />

            <Button
                android:id="@+id/txt3"
                android:layout_width="@dimen/sybol_button_width"
                android:layout_height="wrap_content"
                android:layout_column="2"
                android:layout_row="4"
                android:text="3"
                />

            <Button
                android:id="@+id/sub"
                android:layout_width="@dimen/sybol_button_width"
                android:layout_height="wrap_content"
                android:layout_column="3"
                android:layout_row="4"
                android:text="-"
                />

            <Button
                android:id="@+id/post"
                android:layout_width="@dimen/sybol_button_width"
                android:layout_height="wrap_content"
                android:layout_column="4"
                android:layout_gravity="fill_vertical"
                android:layout_row="4"
                android:layout_rowSpan="2"
                android:text="提交"

                />

            <Button
                android:id="@+id/txt0"
                android:layout_width="@dimen/sybol_button_width2"
                android:layout_height="wrap_content"
                android:layout_column="0"
                android:layout_columnSpan="2"
                android:layout_gravity="fill_horizontal"
                android:layout_row="5"
                android:text="0"
                />

            <Button
                android:id="@+id/txtspl"
                android:layout_width="@dimen/sybol_button_width"
                android:layout_height="wrap_content"
                android:layout_column="2"
                android:layout_row="5"
                android:text="."

            />

            <Button
                android:id="@+id/add"
                android:layout_width="@dimen/sybol_button_width"
                android:layout_height="wrap_content"
                android:layout_column="3"
                android:layout_row="5"
                android:text="+"

            />

        </GridLayout>


</LinearLayout>
